admins = [
    env('ADMIN_JID')
]
'config-type' = 'default'
debug = [ 'server' ]
'default-virtual-host' = env('VIRTUAL_HOST')
dataSource () {
    default () {
        uri = 'jdbc:postgresql://' + env('DB_HOST') + '/' + env('DB_NAME') + '?user=' + env('DB_USER_NAME') + '&password=' + env('DB_USER_PASSWORD') + '&useSSL=false'
        'pool-size' = env('DB_POOL_SIZE')
    }
}
bosh (active: false) {}
http () {
    setup () {
        'admin-password' = env('DB_USER_NAME')
        'admin-user' = env('DB_USER_PASSWORD')
    }
}
'message-archive' (active: false) {}
muc (active: false) {}
pubsub (active: false) {
    trusted = [ 'http@{clusterNode}' ]
}
s2s (active: false) {}
'sess-man' () {
    'message-archive-xep-0136' (active: false) {}
    'message-carbons' (active: false) {}
    pep (active: false) {}
    'spam-filter' (active: false) {}
    'urn:xmpp:csi:0' (active: false) {}
    'urn:xmpp:mam:1' (active: false) {}
    'urn:xmpp:push:0' (active: false) {}
}
ws2s (active: false) {}

c2s {
    'urn:xmpp:sm:3' {
        'resumption-timeout' = 0
    }
}
'hardened-mode' = relaxed
'cluster-mode' = true
# it will be filled by the startup script
'cluster-nodes' = [ ]
